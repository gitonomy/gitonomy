<!DOCTYPE html>
<html>
    <title>{% block title '' %}</title>
    <head>
        <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
        {% stylesheets filter="cssrewrite" combine=true output="css/all.css"
            "bundles/gitonomywebsite/vendor/bootstrap/css/bootstrap.css"
            "bundles/gitonomywebsite/vendor/bootstrap/css/bootstrap-responsive.css"
            "bundles/gitonomywebsite/vendor/codemirror/lib/codemirror.css"
            "bundles/gitonomywebsite/vendor/codemirror/theme/elegant.css"
            "bundles/gitonomywebsite/css/global.sass"
            "bundles/gitonomywebsite/css/layout.sass"
            "bundles/gitonomywebsite/css/splash.sass"
            "bundles/gitonomywebsite/css/codemirror.sass"
            "bundles/gitonomywebsite/css/fonts.sass"
            "bundles/gitonomywebsite/css/preferences/sshKeys.sass"
            "bundles/gitonomywebsite/css/project/newsfeed.sass"
            "bundles/gitonomywebsite/css/project/history.sass"
            "bundles/gitonomywebsite/css/project/source.sass"
            "bundles/gitonomywebsite/css/project/commit.sass"
        %}
            <link rel="stylesheet" href="{{ asset(asset_url) }}" />
        {% endstylesheets %}
        {% block before_head_end '' %}
    </head>
    <body class="{% block body_class '' %}">
        {% block body '' %}

        {% javascripts combine=true output="js/all.js"
            "bundles/gitonomywebsite/vendor/jquery-1.7.1.min.js"
            "bundles/gitonomywebsite/js/d3.v2.js"
            "bundles/gitonomywebsite/vendor/jquery.masonry.min.js"
            "bundles/gitonomywebsite/vendor/bootstrap/js/bootstrap.js"
            "bundles/gitonomywebsite/vendor/codemirror/lib/codemirror.js"
            "bundles/gitonomywebsite/vendor/codemirror/lib/util/loadmode.js"
            "bundles/gitonomywebsite/js/post-link.js"
            "bundles/gitonomywebsite/js/button-groups.js"
            "bundles/gitonomywebsite/js/history.js"
            "bundles/gitonomywebsite/js/commit.js"
            "bundles/gitonomywebsite/js/codemirror.js"
        %}
            <script type="text/javascript" src="{{ asset(asset_url) }}"></script>
        {% endjavascripts %}
        {% block before_body_end '' %}
    </body>
</html>
