<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="gitonomy_twig.git.data_collector.class">Gitonomy\Bundle\TwigBundle\DataCollector\GitDataCollector</parameter>
    </parameters>

    <services>
        <service id="gitonomy_twig.git.data_collector" class="%gitonomy_twig.git.data_collector.class%">
            <tag name="data_collector" id="git" template="GitonomyTwigBundle:Profiler:git.html.twig" />
        </service>
    </services>
</container>
